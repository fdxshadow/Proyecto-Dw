<div id="SupervisorModal" class="modal fade" role="dialog">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <div class="modal-title" style="text-align:center">
          <strong> Agregar supervisor </strong>
        </div>
      </div>
      <div class="modal-body" data-backdrop="static" data-keyboard="false">

        <form id="formulario" onsubmit="return validarFormSup()" action="/test" method="POST">

          <div class="input-group" style="width:100%">
            <span class="input-group-addon addonSup">Nombre</span>
            <input onkeypress="return soloLetras(event)" onchange="validarNombreSup()" type="text" class="form-control" id="nombreSup"
              name="nombreSup" placeholder="Ingrese su nombre" required>
          </div>
          <i style="color:red" id="errorNomSup" class="control-label"></i>

          <br>
          <br>
          <div class="input-group" style="width:100%">
            <span class="input-group-addon addonSup" id="basic-addon1">RUT</span>
            <input onkeypress="return digitosRut(event);" class="form-control" id="rutSup" name="rutSup" onchange="validarRut()" required>
          </div>
          <i style="color:red" id="errorRutSup" class="control-label"></i>
          <br/>
          <i class="textoNaranjoInfo"><i class="fa fa-info-circle"></i> (Ejemplo: 18234542-2)</i>

          <br>
          <br>
          <div class="input-group" style="width:100%">
            <span class="input-group-addon addonSup" id="basic-addon1">Correo</span>
            <input type="email" class="form-control" id="correo" name="correo" placeholder="Username" aria-describedby="basic-addon1"
              required>
          </div>
          <br>
          <br>
          <div class="input-group" style="width:100%">
            <span class="input-group-addon addonSup" id="basic-addon1">Telefono</span>
            <input onkeypress="return onlyNumbers(event);" id="telefono" name="telefono" class="form-control" placeholder="Username"
              aria-describedby="basic-addon1" required>
          </div>

          <br/>
          <div class="form-group" style="text-align:center">
            <div class="col-md-12">

              <button type="submit" class="btn btn-success" form="formulario">Aceptar
                <i class="fa fa-check" aria-hidden="true"></i>
              </button>

              <button type="reset" class="btn btn-primary">
                <i class="fa fa-trash-o" aria-hidden="true"></i> Limpiar
              </button>
            </div>
          </div>

          <br/>


        </form>
      </div>
      <div class="modal-footer" style="text-align:center">

        <button type="button" class="btn btn-primary" data-dismiss="modal">
                    <i class="fa fa-undo" aria-hidden="true"></i> Volver</button>
      </div>
    </div>
  </div>
</div>

